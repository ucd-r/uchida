package jp.co.ulsystems.rie_uchida.janken;

public class HandRundomTest {

	public static void main(String[] args){
		HandRundom test = new HandRundom();

		for(int i=0; i<100; i++ ) {
			if(test.chooseHand().equals(HandRundom.GU) || test.chooseHand().equals(HandRundom.CHOKI) || test.chooseHand().equals(HandRundom.PA)) {
			}else {
				System.out.println(" Error :" + test.chooseHand());
			}
		}

	}
}

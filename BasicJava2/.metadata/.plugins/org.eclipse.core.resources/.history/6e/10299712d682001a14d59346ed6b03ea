		for(int i=0; i<100; i++ ) {
			if(test.chooseHand().equals(Hand.GU) || test.chooseHand().equals(Hand.CHOKI) || test.chooseHand().equals(Hand.PA)) {
			}else {
				System.out.println(" Error :" + test.chooseHand());
			}
		}

	}
}
